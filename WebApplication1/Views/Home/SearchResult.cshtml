@using WebApplication1.Models
@model IEnumerable<Entity>

@{
	if (Model.Count() > 0)
	{
		foreach (Entity entity in Model)
		{
			if (entity.Fields != null && entity.Fields.Count > 0)
			{
				@Html.DisplayTextFor(o => entity.Name)
				<pre><code class="xml"> @entity.XmlString</code></pre>
			}
		}
	}
}
